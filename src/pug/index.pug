doctype html
html(lang="ja")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title webpack dev env
    link(rel="stylesheet", href="../assets/css/app.css")
  body
    div.l-wrapper

      include ./include/_header

      include ./include/_sidebar

      //main
      main.l-main
        .contents
          .contents__container
            .contents__header
              h2.heading-1 Components
            .contents__body
              .grid
                - for (var i = 0; i < 9; i++)
                  div.grid__item
                    article.card
                      .card__body
                        h3.card__title Cards
                      figure.card__image
                        img(src="../assets/img/card_sample.webp", alt="", srcset="")








      include ./include/_footer
